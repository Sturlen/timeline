{
  "name": "@sturlen/timeline",
  "version": "1.1.0",
  "description": "Tweening lib",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "coverage": "jest --coverage",
    "lint": "eslint --ignore-path .gitignore .",
    "prebuild": "npm run lint",
    "build": "tsc",
    "build:debug": "tsc --build debug.tsconfig.json",
    "test": "jest"
  },
  "keywords": [
    "tween",
    "anim"
  ],
  "author": "SNS",
  "license": "ISC",
